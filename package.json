{
  "name": "raptor-js",
  "version": "1.0.0",
  "description": "simple Typescript framework project inspired by express",
  "main": "public/main.ts",
  "keywords": [
    "typescript",
    "javascript",
    "node",
    "nodejs",
    "express",
    "nestjs",
    "framework",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Pipe930/raptor-framework"
  },
  "bugs": {
    "url": "https://github.com/Pipe930/raptor-framework/issues"
  },
  "homepage": "https://github.com/Pipe930/raptor-framework#readme",
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=24.13.0"
  },
  "scripts": {
    "exec": "ts-node public/main.ts",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "dev": "NODE_ENV=development nodemon -r tsconfig-paths/register public/main.ts",
    "start": "node dist/public/index.js",
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "jest --watch --onlyChanged",
    "test:cov": "jest --coverage",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
  },
  "author": "Pipe930",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@jest/globals": "30.2.0",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.8",
    "eslint": "9.39.2",
    "globals": "17.2.0",
    "jest": "30.2.0",
    "nodemon": "3.1.11",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0"
  }
}
