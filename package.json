{
  "name": "skyguard-js",
  "version": "1.1.2",
  "description": "A lightweight, dependency-free TypeScript backend framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./helpers": {
      "types": "./dist/helpers/http.d.ts",
      "import": "./dist/helpers/http.js"
    },
    "./middlewares": {
      "types": "./dist/middlewares/index.d.ts",
      "import": "./dist/middlewares/index.js"
    },
    "./exceptions": {
      "types": "./dist/exceptions/httpExceptions.d.ts",
      "import": "./dist/exceptions/httpExceptions.js"
    },
    "./security": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.js"
    }
  },
  "keywords": [
    "typescript",
    "javascript",
    "node",
    "nodejs",
    "express",
    "nestjs",
    "framework",
    "library",
    "backend",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Pipe930/Skyguard-js"
  },
  "bugs": {
    "url": "https://github.com/Pipe930/Skyguard-js/issues"
  },
  "homepage": "https://github.com/Pipe930/Skyguard-js#readme",
  "engines": {
    "node": ">=24.13.0"
  },
  "files": [
    "dist",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "scripts": {
    "exec": "ts-node examples/main.ts",
    "lint": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "dev": "NODE_ENV=development nodemon examples/main.ts",
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "jest --watch --onlyChanged",
    "test:cov": "jest --coverage",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "author": {
    "name": "Pipe930",
    "url": "https://github.com/Pipe930"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@jest/globals": "30.2.0",
    "@types/jest": "30.0.0",
    "@types/node": "25.3.0",
    "eslint": "10.0.2",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.3.0",
    "jest": "30.2.0",
    "nodemon": "3.1.14",
    "prettier": "3.8.1",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1"
  }
}
